extends layout
append header
append body
block content


           
    // Page Content
    .container
      // Page Heading/Breadcrumbs
      h1.mt-4.mb-3
      ol.breadcrumb
        li.breadcrumb-item
          a(href="/") Home
        li.breadcrumb-item.active Contact
      h1 Location
      // Content Row
      .row
        // Map Column
        .col-lg-8.mb-4
          // Embedded Google Map
      
          iframe(width="100%" height="400px" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://www.google.com/maps/embed/v1/place?q=university%20of%20york%20computer%20science&key=AIzaSyD6yHSoDkPpP-eKCNrG2wxdqqXKG-1Sa_g" allowfullscreen)
    
        // Contact Details Column
        .col-lg-4.mb-4
          h3 Contact Details
          p
            abbr(title="Email") 
            | Email : 
            a(href="mailto:cyber@yusu.org")
              | cyber@yusu.org
    

    
        
