extends layout

block content
    
    h1= message
    h2= error.status
    pre #{error.stack}

    // Page Content
    .container

      // Page Heading/Breadcrumbs
      h1.mt-4.mb-3
        |  404
        small   Page Not Found

      ol.breadcrumb
        li.breadcrumb-item
          a(href="/") Home
        li.breadcrumb-item.active 404
      
      img(src='/images/404Image.jpg',style="position:absolute; left:25% ; z-index: -1")
      
    // /.container
  




